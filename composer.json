{
  "name": "clubnix/votix",
  "type": "project",
  "version": "5.0",
  "description": "Votix. The advanced and secure online voting platform.",
  "license": "MIT",
  "authors": [
    {
      "name": "Philippe Lewin",
      "email": "philippe.lewin@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "classmap": [
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\Unit\\": "tests/unit",
      "App\\Tests\\Acceptance\\": "tests/acceptance",
      "App\\Tests\\": "tests/_support",
      "Helper\\": "tests/_support/Helper/",
      "_generated\\": "tests/_support/_generated/"
    }
  },
  "require": {
    "php": ">=7.1",
    "ext-pdo_sqlite": "*",
    "ext-json": "*",
    "symfony/monolog-bundle": "^3.3",
    "symfony/swiftmailer-bundle": "^3.2",
    "sensio/framework-extra-bundle": "^5.2",
    "doctrine/orm": "^2.6",
    "doctrine/doctrine-bundle": "^1.8",
    "doctrine/doctrine-cache-bundle": "^1.3",
    "sensiolabs/security-checker": "^5.0",
    "aws/aws-sdk-php": "3.*",
    "zendframework/zend-crypt": "^3.3",
    "symfony/flex": "^1.2",
    "symfony/asset": "^4.2",
    "symfony/orm-pack": "^1.0",
    "symfony/twig-bundle": "^4.2",
    "symfony/form": "^4.2",
    "symfony/security-bundle": "^4.2",
    "symfony/translation": "^4.2",
    "symfony/validator": "^4.2",
    "symfony/web-server-bundle": "^4.2",
    "symfony/debug-pack": "^1.0",
    "symfony/webpack-encore-bundle": "^1.2",
    "easycorp/easyadmin-bundle": "^2.1"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^7",
    "symfony/phpunit-bridge": "^4.0",
    "codeception/codeception": "^2.5",
    "symfony/dotenv": "^4.2",
    "symfony/maker-bundle": "^1.11",
    "doctrine/doctrine-fixtures-bundle": "^3.1",
    "symfony/profiler-pack": "^1.0",
    "vimeo/psalm": "^3.2"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "security-checker security:check": "script"
    }
  }
}
