{
    "name": "clubnix/votix",
    "type": "project",
    "version": "5.0",
    "description": "Votix. The advanced and secure online voting platform.",
    "license": "MIT",
    "authors": [
        {
            "name": "Philippe Lewin",
            "email": "philippe.lewin@gmail.com"
        },
        {
            "name": "Club*Nix",
            "email": "club.nix@edu.esiee.fr"
        }
    ],
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*",
        "laminas/laminas-zendframework-bridge": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "classmap": []
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\Unit\\": "tests/unit",
            "App\\Tests\\Acceptance\\": "tests/acceptance",
            "App\\Tests\\": "tests/_support",
            "Helper\\": "tests/_support/Helper/",
            "_generated\\": "tests/_support/_generated/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1",
        "ext-pdo_sqlite": "*",
        "ext-json": "*",
        "symfony/monolog-bundle": "^3.3",
        "sensio/framework-extra-bundle": "^5.2",
        "sensiolabs/security-checker": "^6.0",
        "aws/aws-sdk-php": "3.*",
        "symfony/flex": "^1.5",
        "symfony/asset": "^5.1",
        "symfony/orm-pack": "^2.0",
        "symfony/twig-bundle": "^5.1",
        "symfony/form": "^5.1",
        "symfony/security-bundle": "^5.1",
        "symfony/translation": "^5.1",
        "symfony/validator": "^5.1",
        "symfony/debug-pack": "^1.0",
        "symfony/webpack-encore-bundle": "^1.2",
        "symfony/serializer-pack": "^1.0",
        "laminas/laminas-crypt": "^3.3"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9",
        "codeception/codeception": "^4.0",
        "symfony/dotenv": "^5.1",
        "symfony/maker-bundle": "^1.11",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "symfony/profiler-pack": "^1.0",
        "vimeo/psalm": "^3.2",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-symfony": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.1.*"
        }
    }
}
